{
    "name": "valksor/php-dev-cs-fixer-custom-fixers",
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": ">=8.4",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/finder": "*",
        "valksor/functions-preg": "^1.0"
    },
    "conflict": {
        "friendsofphp/php-cs-fixer": "<3.81.0"
    },
    "suggest": {
        "doctrine/migrations": "To fix doctrine migration files"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "ValksorDev\\PhpCsFixerCustomFixers\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
